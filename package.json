    "micro": "^9.0.0",
    "micro-cacheable": "^2.0.1",
    "micro-route": "^2.4.0",
    "nuxt": "^1.4.1",
    "nuxt-i18n": "^4.0.0",
    "underscore": "^1.9.1",
    "wavesurfer.js": "^2.0.6"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "csv": "^3.1.0",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "nodemon": "^1.11.0",
