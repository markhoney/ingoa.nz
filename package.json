{
	"name": "ingoa.nz",
	"version": "1.0.0",
	"description": "Ngā Ingoa o Aotearoa: An oral dictionary of Māori placenames",
	"author": "Mark Honeychurch",
	"private": true,
	"scripts": {
		"db": "node src/server/db/import.js",
		"images": "node static/img/staticmaps.js",
		"share": "lt --port 3000 --subdomain ingoa",
		"kill": "taskkill /F /im node.exe",
		"dev": "cross-env NODE_OPTIONS='--max-old-space-size=8192' NODE_ENV=development nodemon src/server/index.js --watch src/server --watch src/apollo-server --ext js,graphql",
		"devsimple": "nuxt",
		"chrome": "cross-env NODE_OPTIONS='--max-old-space-size=4096' NODE_ENV=development nodemon --inspect src/server/index.js --watch src/server",
		"build": "nuxt build --modern",
		"analyse": "nuxt build --modern --analyze",
		"startold": "cross-env NODE_ENV=production node src/server/index.js",
		"start": "nuxt start --modern",
		"pm2": "pm2 start npm ecosystem.config.js --name ingoa -- start",
		"generate": "./node_modules/bin/nuxt generate"
	},
	"dependencies": {
		"@nuxtjs/apollo": "^4.0.0-rc4",
		"@nuxtjs/google-analytics": "^2.2.0",
		"@nuxtjs/pwa": "^2.6.0",
		"@nuxtjs/sitemap": "^0.2.2",
		"apollo-cache-inmemory": "^1.5.1",
		"apollo-cache-persist": "^0.1.1",
		"apollo-server-express": "^2.4.8",
		"core-js": "2",
		"express": "^4.16.4",
		"graphql-import": "^0.7.1",
		"graphql-tag": "^2.10.1",
		"graphql-tools": "^4.0.4",
		"image-map": "^1.1.5",
		"jquery": "^3.3.1",
		"js-levenshtein": "^1.1.6",
		"lodash": "^4.17.11",
		"nuxt": "^2.6.1",
		"nuxt-i18n": "^5.8.5",
		"nuxt-leaflet": "0.0.18",
		"nuxt-trailingslash-module": "^1.1.0",
		"stylus": "^0.54.5",
		"stylus-loader": "^3.0.2",
		"vue-object-merge": "^0.1.7",
		"vue2-google-maps": "^0.10.6",
		"vuetify": "^1.5.9",
		"vuetify-loader": "^1.2.1",
		"wavesurfer.js": "^2.2.1"

	},
	"devDependencies": {
		"@babel/runtime-corejs2": "^7.4.3",
		"cross-env": "^5.2.0",
		"csv": "^5.1.1",
		"express-nedb-rest": "^1.3.0",
		"JSONStream": "^1.3.5",
		"localtunnel": "^1.9.1",
		"nedb": "^1.8.0",
		"nedb-async": "0.0.9",
		"nodemon": "^1.18.10"
	}
}
