    "lodash": "^4.17.10",
    "lokijs": "^1.5.5",
    "micro": "^9.0.0",
    "micro-cacheable": "^2.0.1",
    "micro-route": "^2.4.0",
    "nuxt": "^1.4.1",
    "nuxt-i18n": "^4.1.0",
    "nuxt-trailingslash-module": "^1.1.0",
    "underscore": "^1.9.1",
