{
	"name": "ingoa.nz",
	"version": "1.0.0",
	"description": "Ngā Ingoa o Aotearoa: An oral dictionary of Māori placenames",
	"author": "Mark Honeychurch",
	"private": true,
	"scripts": {
		"prep": "run-s prep:nominatim prep:data:import prep:data:connect prep:nominatim:stop",
		"prep:tools": "npm install --global --production windows-build-tools",
		"prep:data:import": "node src/data/import.js",
		"prep:data:connect": "node src/data/connect.js",
		"prep:audio": "node src/data/splitaudio.js",
		"prep:nominatim": "docker start nominatim",
		"prep:nominatim:stop": "docker stop nominatim",
		"prep:images": "node src/data/images.js",
		"explore": "gridsome explore",
		"dev": "gridsome develop",
		"build": "gridsome build",
		"serve": "http-server docs",
		"info": "gridsome info",
		"dev:share": "lt --port 8000 --subdomain ingoa",
		"static:host": "BASE_URL=http://localhost:80 caddy -agree -conf Caddyfile.static",
		"upgrade:major": "yarn upgrade-interactive --latest"
	},
	"devDependencies": {
		"@gridsome/plugin-sitemap": "^0.4.0",
		"appmetrics-dash": "^5.3.0",
		"axios": "^0.27.2",
		"bootstrap-vue": "^2.22.0",
		"cheerio": "^1.0.0-rc.12",
		"cross-env": "^7.0.3",
		"distinct-colors": "^3.0.0",
		"dotenv": "^16.0.1",
		"googleapis": "^104.0.0",
		"gridsome": "^0.7.23",
		"gridsome-plugin-feed": "^1.0.2",
		"gridsome-plugin-flexsearch": "^2.0.2",
		"gridsome-plugin-i18n": "^1.6.0",
		"gridsome-plugin-pwa": "^0.0.22",
		"image-map": "^2.0.1",
		"jimp": "^0.16.1",
		"jquery": "^3.6.0",
		"js-levenshtein": "^1.1.6",
		"lodash": "^4.17.21",
		"media-split": "^5.4.0",
		"node-env-run": "^4.0.2",
		"node-fetch": "^2.6.7",
		"nodemon": "^2.0.18",
		"npm-run-all": "^4.1.5",
		"pluralize": "^8.0.0",
		"rimraf": "^3.0.2",
		"tar": "^6.1.11",
		"vue-fragment": "^1.6.0",
		"vue-object-merge": "^1.0.0",
		"vue2-google-maps": "^0.10.7",
		"wavesurfer.js": "^6.2.0"
	}
}
