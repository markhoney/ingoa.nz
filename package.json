{
  "name": "ingoa",
  "version": "0.0.0",
  "description": "Nuxt.js + Î” micro project",
  "author": "Mark Honeychurch <mark@honeychurch.org>",
  "private": true,
  "scripts": {
    "import": "node db/import.js",
    "connect": "node db/connect.js",
    "images": "node static/media/images/staticmaps.js",
